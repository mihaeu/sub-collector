{
    "name": "mihaeu/sub-collector",
    "description": "Sub-Collector is a console based app written in PHP, its main purpose is to batch collect subtitles.",
    "keywords": ["subtitle", "SubDB", "movie"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Haeuslmann",
            "email": "haeuslmann@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "symfony/console":          "2.*",
        "php":                      ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit":          "4.*",
        "mikey179/vfsStream":       "1.*",
        "mockery/mockery":          "0.*",
        "satooshi/php-coveralls":   "~0.6"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Mihaeu\\SubCollector\\": ["src/", "tests/unit/"]
        }
    },
    "bin": [
        "bin/sub-collector"
    ]
}
